{
  "name": "csncjs-pendant-numpad",
  "description": "A simple logilink numpad pendant for cncjs.",
  "keywords": [
    "cncjs",
    "pendant",
    "keyboard"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "contributors": [
    "Nelio Santos <nelio@e01aio.com.br",
    "Mario Lukas <info@mariolukas.de>",
    "Per Ivar Nerseth <perivar@nerseth.com>"
  ],
  "homepage": "https://github.com/perivar/cncjs-pendant-numpad#readme",
  "bugs": {
    "url": "https://github.com/perivar/cncjs-pendant-numpad/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perivar/cncjs-pendant-numpad.git"
  },
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "bin": "bin/cncjs-pendant-numpad.js",
  "preferGlobal": true,
  "scripts": {
    "build-ts": "tsc",
    "copy-files": "cpy 'src/*.json' lib",
    "build": "rimraf ./lib && npm run build-ts && npm run copy-files",
    "start:dev": "nodemon",
    "start": "npm run build && node bin/cncjs-pendant-numpad.js",
    "lint": "eslint '**/*.{js,ts}'",
    "lint-fix": "eslint '**/*.{js,ts}' --fix",
    "format": "prettier --check \"**/*.{js,ts}\"",
    "format-fix": "prettier --write \"**/*.{js,ts}\"",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "commander": "^7.0.0",
    "inquirer": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-hid": "^2.1.2",
    "npmlog": "^5.0.0",
    "serialport": "^9.0.0",
    "socket.io-client": "^2.5.0",
    "deepmerge": "^4.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "^7.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^10.0.0",
    "@types/node-hid": "^1.3.1",
    "@types/npmlog": "^4.1.4",
    "@types/serialport": "^8.0.2",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "cpy-cli": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^1.0.0",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-node": "^9.0.0",
    "typescript": "~4.4.0"
  }
}
